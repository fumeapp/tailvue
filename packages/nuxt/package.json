{
  "name": "@tailvue/nuxt",
  "version": "0.1.68",
  "description": "Nuxt module for tailvue",
  "private": false,
  "keywords": [
    "tailvue",
    "vue3",
    "vue-3",
    "vue",
    "vue-plugin",
    "tailwindcss",
    "windicss",
    "nuxt-module",
    "nuxt",
    "nuxt3"
  ],
  "homepage": "https://github.com/fumeapp/tailvue#readme",
  "bugs": {
    "url": "https://github.com/fumeapp/tailvue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fumeapp/tailvue.git",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "author": "Kevin Olson <acidjazz@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/runtime/plugin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "pathe": "^0.2.0",
    "tailvue": "^0.1.30"
  },
  "peerDependencies": {
    "tailvue": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "nuxt3": "latest",
    "unbuild": "^0.6.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
