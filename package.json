{
  "name": "tailvue",
  "version": "0.1.57",
  "description": "windicss and tailwindcss components for Vue",
  "repository": "https://github.com/fumeapp/tailvue",
  "author": "Kevin Olson",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue3",
    "vue-3",
    "vue",
    "vue-plugin",
    "windicss",
    "tailwindcss"
  ],
  "files": [
    "dist",
    "src/types/*",
    "src/typings.d.ts",
    "web-types"
  ],
  "types": "src/typings.d.ts",
  "main": "./dist/tailvue.umd.js",
  "module": "./dist/tailvue.es.js",
  "exports": {
    ".": {
      "import": "./dist/tailvue.es.js",
      "require": "./dist/tailvue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite playground",
    "build": "vite build"
  },
  "dependencies": {
    "vue": "^3.2.21"
  },
  "peerDependencies": {
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@iconify/vue": "^3.1.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@types/node": "^17.0.18",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vueuse/core": "^7.6.2",
    "simple-syntax-highlighter": "^2.2.0",
    "tsup": "^5.7.0",
    "unbuild": "^0.6.9",
    "vite": "^2.6.14",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  },
  "web-types": "web-types/web-types.json"
}
